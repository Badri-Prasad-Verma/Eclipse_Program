package app_java_1;

import java.util.Scanner;

public class A {
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		System.out.println("Enter any number : ");
		int n = scan.nextInt();
		if(n%3==0) {
			int square= n*n;
			System.out.println(n +" square of the number is : "+square);
		}else if(n%5==0) {
			double root=Math.sqrt(n);
			System.out.println(n+" the root of the number is : "+root);
		}else if(n%3==0 && n%5==0) {
			int cubes= n*n*n;
			System.out.println(n +" cubes of the number is : "+cubes);
		}
	}
}








