package app_java_1;

import java.util.Scanner;

public class D{
	public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.print("Enter a number (n): ");
        int n = scanner.nextInt();

        for (int i = 1; i <= n; i++) {
            if (i % 3 == 0) {
                if (i % 5 == 0) {
                    int cube = i * i * i;
                    System.out.println(i + " is divisible by both 3 and 5. Cube: " + cube);
                } else {
                    int square = i * i;
                    System.out.println(i + " is divisible by 3. Square: " + square);
                }
            }
        }
}
